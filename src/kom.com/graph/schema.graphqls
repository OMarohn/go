# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Query {
  coasters: [Coaster!]!
}

type Coaster {
	id: ID!
	name: String!
	manufacture: String
	height: Int
}

input NewCoaster {
	name: String!
	manufacture: String
	height: Int
}

type Mutation {
  createCoaster(input: NewCoaster!): Coaster!
}
